{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "radix": "off",
      "no-console": "off",
      "consistent-return": "off",
      "no-underscore-dangle": "off",
      "spaced-comment": "off",
      "no-unused-vars": ["error", { "argIgnorePattern": "req|res|next|val" }]
    }
}